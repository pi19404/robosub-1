{
    "sensor/vision/visiond": {
        "port": 20054,
        "listen": [],
        "release": {"path": null},
        "mock": {"path": null},
        "maintenance_interval": 5,
        "vision_processors": [
            "sensor/vision/cam_fake"
        ]
    },
    "sensor/vision/cam_fake: {
        "image": "/home/cevans/Pictures/test_im.jpg",
    },
    "sensor/vision/cam_front": {
        "stream_port": 50000,
        "symlink": "/dev/cam_front",
        "codec" : "MJPG",
        "enable": true,
        "log": [
            "raw",
            "processed"
        ],
        "fps": 30,
        "width": 640,
        "height": 480,
        "plugins": []
    },
    "sensor/vision/cam_down": {
        "stream_port": 50001,
        "symlink": "/dev/cam_down",
